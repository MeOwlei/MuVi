#include<stdio.h>
#include<raylib.h>

int main(void){

    Music song = LoadMusicStream("./");
    InitWindow(800, 600, "Music Visualizeir");

    while (!WindowShouldClose()) {
        BeginDrawing();
        ClearBackground(PURPLE);
        EndDrawing();
    }
    CloseWindow();

    return 0;
}

